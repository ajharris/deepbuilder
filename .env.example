# .env.example
# Copy this file to .env and fill in your secrets and config values.

# Flask config
FLASK_ENV=development
FLASK_DEBUG=1
FLASK_APP=backend.app

# Secret key (used for sessions, JWT, CSRF, etc.)
SECRET_KEY=your-very-secret-key
TEST_KEY=not-very-secret-key

# Database (if you're using SQLAlchemy later)
DATABASE_URL=sqlite:///db.sqlite3

# CORS whitelist (optional)
CORS_ORIGINS=http://localhost:3000

# Example config for future use (e.g., MONAI model path)
MODEL_DIR=./models
UPLOAD_DIR=./uploads

# GitHub integration (do not commit real tokens)
GITHUB_TOKEN=your-github-token
GITHUB_REPO=yourusername/yourrepo
